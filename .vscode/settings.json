// Place your settings in this file to overwrite default and user settings.
{
  "workbench.iconTheme": "vscode-icons",
  "editor.tabSize": 2,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/bower_componentes/**": true,
    "**/vendor/**": true,
    "**/platforms/**": true,
    "**/plugins/**": true
  },
  "terminal.integrated.fontSize": 12,
  "editor.fontSize": 14
}
